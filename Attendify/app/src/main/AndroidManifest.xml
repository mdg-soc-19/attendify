<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.SOC.attendify">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".ActivityLogin"></activity>
        <activity android:name=".Stats_activity"></activity>
        <activity android:name=".preplanning_activity"></activity>
        <activity android:name=".Notification_activity"></activity>
        <activity android:name=".Aboutus_activity"></activity>
        <activity android:name=".Rescheduled_activity"></activity>
        <activity android:name=".Stats_1_Activity"></activity>
        <activity android:name=".ResetPasswordActivity"></activity>
        <activity android:name=".Subjects"></activity>
        <activity android:name=".S1"></activity>
        <activity android:name=".S2"></activity>
        <activity android:name=".S3"></activity>
        <activity android:name=".S4"></activity>
        <activity android:name=".S5"></activity>
        <activity android:name=".S6"></activity>
        <activity android:name=".S7"></activity>
<activity android:name=".Faculty_login"></activity>
        <activity android:name=".Rescheduledstudent"></activity>
        <activity android:name=".Imageselector"></activity>
        <activity android:name=".Verify"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <service
            android:name=".AlarmService"
            android:enabled="true" />

        <receiver android:name=".AlarmReceiver"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
